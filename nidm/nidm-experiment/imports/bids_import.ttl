bids:ScanOptions rdf:type owl:DatatypeProperty ;

            rdfs:label "ScanOptions"^^xsd:string ;

            dct:description "RECOMMENDED. Parameters of ScanningSequence. Corresponds to DICOM Tag 0018, 0022 Scan Options."^^xsd:string ;

            bids:ScanOptions owl:sameAs dicom:dicom_00180022 ;

            obo:IAO_0000116 "To be discussed"^^xsd:string .



###  http://purl.org/nidash/bids#SequenceName

bids:SequenceName rdf:type owl:DatatypeProperty ;

            rdfs:label "SequenceName"^^xsd:string ;

            dct:description "RECOMMENDED. Manufacturer’s designation of the sequence name. Corresponds to DICOM Tag 0018, 0024 Sequence Name."^^xsd:string ;

            bids:SequenceName owl:sameAs dicom:dicom_00180024 ;

            obo:IAO_0000116 "To be discussed"^^xsd:string .



###  http://purl.org/nidash/bids#PulseSequenceDetails

bids:PulseSequenceDetails rdf:type owl:DatatypeProperty ;

            rdfs:label "PulseSequenceDetails"^^xsd:string ;

            dct:description "RECOMMENDED. Information beyond pulse sequence type that identifies the specific pulse sequence used (i.e. Standard Siemens Sequence distributed with the VB17 software, Siemens WIP ### version #.##, or Sequence written by X using a version compiled on MM/DD/YYYY)."^^xsd:string ;

            obo:IAO_0000116 "To be discussed"^^xsd:string .



###  http://purl.org/nidash/bids#NonlinearGradientCorrection

bids:NonlinearGradientCorrection rdf:type owl:DatatypeProperty ;

            rdfs:label "NonlinearGradientCorrection"^^xsd:string ;

            dct:description "RECOMMENDED. Boolean stating if the image saved has been corrected for gradient nonlinearities by the scanner sequence."^^xsd:string ;

            obo:IAO_0000116 "To be discussed"^^xsd:string .



###  http://purl.org/nidash/bids#NumberShots

bids:NumberShots rdf:type owl:DatatypeProperty ;

            rdfs:label "NumberShots"^^xsd:string ;

            dct:description "RECOMMENDED. The number of RF excitations need to reconstruct a slice or volume. Please mind that this is not the same as Echo Train Length which denotes the number of lines of k-space collected after an excitation."^^xsd:string ;

            obo:IAO_0000116 "To be discussed"^^xsd:string .



###  http://purl.org/nidash/bids#ParallelReductionFactorInPlane

bids:ParallelReductionFactorInPlane rdf:type owl:DatatypeProperty ;

            rdfs:label "ParallelReductionFactorInPlane"^^xsd:string ;

            dct:description "RECOMMENDED. The parallel imaging (e.g, GRAPPA) factor. Use the denominator of the fraction of k-space encoded for each slice. For example, 2 means half of k-space is encoded. Corresponds to DICOM Tag 0018, 9069 Parallel Reduction Factor In-plane."^^xsd:string ;

            bids:ParallelReductionFactorInPlane owl:sameAs dicom:dicom_00189069 ;

            obo:IAO_0000116 "To be discussed"^^xsd:string .



###  http://purl.org/nidash/bids#ParallelAcquisitionTechnique

bids:ParallelAcquisitionTechnique rdf:type owl:DatatypeProperty ;

            rdfs:label "ParallelAcquisitionTechnique"^^xsd:string ;

            dct:description "RECOMMENDED. The type of parallel imaging used (e.g. GRAPPA, SENSE). Corresponds to DICOM Tag 0018, 9078 Parallel Acquisition Technique."^^xsd:string ;

            bids:ParallelAcquisitionTechnique owl:sameAs dicom:dicom_00189078 ;

            obo:IAO_0000116 "To be discussed"^^xsd:string .



###  http://purl.org/nidash/bids#PartialFourier

bids:PartialFourier rdf:type owl:DatatypeProperty ;

            rdfs:label "PartialFourier"^^xsd:string ;

            dct:description "RECOMMENDED. The fraction of partial Fourier information collected. Corresponds to DICOM Tag 0018, 9081 Partial Fourier."^^xsd:string ;

            bids:PartialFourier owl:sameAs dicom:dicom_00189081 ;

            obo:IAO_0000116 "To be discussed"^^xsd:string .



###  http://purl.org/nidash/bids#PartialFourierDirection

bids:PartialFourierDirection rdf:type owl:DatatypeProperty ;

            rdfs:label "PartialFourierDirection"^^xsd:string ;

            dct:description "RECOMMENDED. The direction where only partial Fourier information was collected. Corresponds to DICOM Tag 0018, 9036 Partial Fourier Direction."^^xsd:string ;

            bids:PartialFourierDirection owl:sameAs dicom:dicom_00189036 ;

            obo:IAO_0000116 "To be discussed"^^xsd:string .



###  http://purl.org/nidash/bids#PhaseEncodingDirection

bids:PhaseEncodingDirection rdf:type owl:DatatypeProperty ;

            rdfs:label "PhaseEncodingDirection"^^xsd:string ;

            dct:description "RECOMMENDED. Possible values: i, j, k, i-, j-, k-. The letters i, j, kcorrespond to the first, second and third axis of the data in the NIFTI file. The polarity of the phase encoding is assumed to go from zero index to maximum index unless - sign is present (then the order is reversed - starting from the highest index instead of zero). PhaseEncodingDirection is defined as the direction along which phase is was modulated which may result in visible distortions. Note that this is not the same as the DICOM term InPlanePhaseEncodingDirection which can have ROW or COL values. This parameter is REQUIRED if corresponding fieldmap data is present or when using multiple runs with different phase encoding directions (which can be later used for field inhomogeneity correction)."^^xsd:string ;

            obo:IAO_0000116 "To be discussed"^^xsd:string .



###  http://purl.org/nidash/bids#EffectiveEchoSpacing

bids:EffectiveEchoSpacing rdf:type owl:DatatypeProperty ;

            rdfs:label "EffectiveEchoSpacing"^^xsd:string ;

            dct:description "RECOMMENDED. The effective sampling interval, specified in seconds, between lines in the phase-encoding direction, defined based on the size of the reconstructed image in the phase direction. It is frequently, but incorrectly, referred to as dwell time (see DwellTime parameter below for actual dwell time). It is required for unwarping distortions using field maps. Note that beyond just in-plane acceleration, a variety of other manipulations to the phase encoding need to be accounted for properly, including partial fourier, phase oversampling, phase resolution, phase field-of-view and interpolation.2This parameter is REQUIRED if corresponding fieldmap data is present."^^xsd:string ;

            obo:IAO_0000116 "To be discussed"^^xsd:string .



###  http://purl.org/nidash/bids#TotalReadoutTime

bids:TotalReadoutTime rdf:type owl:DatatypeProperty ;

            rdfs:label "TotalReadoutTime"^^xsd:string ;

            dct:description "RECOMMENDED. This is actually the effective total readout time , defined as the readout duration, specified in seconds, that would have generated data with the given level of distortion. It is NOT the actual, physical duration of the readout train. If EffectiveEchoSpacing has been properly computed, it is just EffectiveEchoSpacing * (ReconMatrixPE - 1).3 . This parameter is REQUIRED if corresponding field/distortion maps acquired with opposing phase encoding directions are present (see 8.9.4)."^^xsd:string ;

            obo:IAO_0000116 "To be discussed"^^xsd:string .



###  http://purl.org/nidash/bids#EchoTime

bids:EchoTime rdf:type owl:DatatypeProperty ;

            rdfs:label "EchoTime"^^xsd:string ;

            dct:description "RECOMMENDED. The echo time (TE) for the acquisition, specified in seconds. This parameter is REQUIRED if corresponding fieldmap data is present or the data comes from a multi echo sequence. Corresponds to DICOM Tag 0018, 0081 Echo Time(please note that the DICOM term is in milliseconds not seconds)."^^xsd:string ;

            bids:EchoTime owl:sameAs dicom:dicom_00180081 ;

            obo:IAO_0000116 "To be discussed"^^xsd:string .



###  http://purl.org/nidash/bids#InversionTime

bids:InversionTime rdf:type owl:DatatypeProperty ;

            rdfs:label "InversionTime"^^xsd:string ;

            dct:description "RECOMMENDED. The inversion time (TI) for the acquisition, specified in seconds. Inversion time is the time after the middle of inverting RF pulse to middle of excitation pulse to detect the amount of longitudinal magnetization. Corresponds to DICOM Tag 0018, 0082 Inversion Time (please note that the DICOM term is in milliseconds not seconds)."^^xsd:string ;

            bids:InversionTime owl:sameAs dicom:dicom_00180082 ;

            obo:IAO_0000116 "To be discussed"^^xsd:string .



###  http://purl.org/nidash/bids#SliceTiming

bids:SliceTiming rdf:type owl:DatatypeProperty ;

            rdfs:label "SliceTiming"^^xsd:string ;

            dct:description "RECOMMENDED. The time at which each slice was acquired within each volume (frame) of the acquisition. Slice timing is not slice order -- rather, it is a list of times (in JSON format) containing the time (in seconds) of each slice acquisition in relation to the beginning of volume acquisition. The list goes through the slices along the slice axis in the slice encoding dimension (see below). Note that to ensure the proper interpretation of the SliceTiming field, it is important to check if the OPTIONAL SliceEncodingDirection exists. In particular, if SliceEncodingDirection is negative, the entries in SliceTiming are defined in reverse order with respect to the slice axis (i.e., the final entry in the SliceTiming list is the time of acquisition of slice 0). This parameter is REQUIRED for sparse sequences that do not have the DelayTimefield set. In addition without this parameter slice time correction will not be possible."^^xsd:string ;

            obo:IAO_0000116 "To be discussed"^^xsd:string .



###  http://purl.org/nidash/bids#SliceEncodingDirection

bids:SliceEncodingDirection rdf:type owl:DatatypeProperty ;

            rdfs:label "SliceEncodingDirection"^^xsd:string ;

            dct:description "RECOMMENDED. Possible values: i, j, k, i-, j-, k- (the axis of the NIfTI data along which slices were acquired, and the direction in which SliceTiming is defined with respect to). i, j, k identifiers correspond to the first, second and third axis of the data in the NIfTI file. A - sign indicates that the contents of SliceTiming are defined in reverse order - that is, the first entry corresponds to the slice with the largest index, and the final entry corresponds to slice index zero. When present, the axis defined by SliceEncodingDirection needs to be consistent with the slice_dim field in the NIfTI header. When absent, the entries in SliceTiming must be in the order of increasing slice index as defined by the NIfTI header."^^xsd:string ;

            obo:IAO_0000116 "To be discussed"^^xsd:string .



###  http://purl.org/nidash/bids#DwellTime

bids:DwellTime rdf:type owl:DatatypeProperty ;

            rdfs:label "DwellTime"^^xsd:string ;

            dct:description "RECOMMENDED. Actual dwell time (in seconds) of the receiver per point in the readout direction, including any oversampling. For Siemens, this corresponds to DICOM field (0019,1018) (in ns). This value is necessary for the optional readout distortion correction of anatomicals in the HCP Pipelines. It also usefully provides a handle on the readout bandwidth, which isn’t captured in the other metadata tags. Not to be confused with EffectiveEchoSpacing, and the frequent mislabeling of echo spacing (which is spacing in the phase encoding direction) as "dwell time" (which is spacing in the readout direction)."^^xsd:string ;

            bids:DwellTime owl:sameAs dicom:dicom_00191018 ;

            obo:IAO_0000116 "To be discussed"^^xsd:string .



###  http://purl.org/nidash/bids#FlipAngle

bids:FlipAngle rdf:type owl:DatatypeProperty ;

            rdfs:label "FlipAngle"^^xsd:string ;

            dct:description "RECOMMENDED. Flip angle for the acquisition, specified in degrees. Corresponds to: DICOM Tag 0018, 1314 Flip Angle."^^xsd:string ;

            bids:FlipAngle owl:sameAs dicom:dicom_00181314 ;

            obo:IAO_0000116 "To be discussed"^^xsd:string .



###  http://purl.org/nidash/bids#MultibandAccelerationFactor

bids:MultibandAccelerationFactor rdf:type owl:DatatypeProperty ;

            rdfs:label "MultibandAccelerationFactor"^^xsd:string ;

            dct:description "RECOMMENDED. The multiband factor, for multiband acquisitions."^^xsd:string ;

            obo:IAO_0000116 "To be discussed"^^xsd:string .



###  http://purl.org/nidash/bids#NegativeContrast

bids:NegativeContrast rdf:type owl:DatatypeProperty ;

            rdfs:label "NegativeContrast"^^xsd:string ;

            dct:description "OPTIONAL. Boolean (true or false) value specifying whether increasing voxel intensity (within sample voxels) denotes a decreased value with respect to the contrast suffix. This is commonly the case when Cerebral Blood Volume is estimated via usage of a contrast agent in conjunction with a T2* weighted acquisition protocol."^^xsd:string ;

            obo:IAO_0000116 "To be discussed"^^xsd:string .



###  http://purl.org/nidash/bids#MultibandAccelerationFactor

bids:MultibandAccelerationFactor rdf:type owl:DatatypeProperty ;

            rdfs:label "MultibandAccelerationFactor"^^xsd:string ;

            dct:description "RECOMMENDED. The multiband factor, for multiband acquisitions."^^xsd:string ;

            obo:IAO_0000116 "To be discussed"^^xsd:string .



###  http://purl.org/nidash/bids#AnatomicalLandmarkCoordinates

bids:AnatomicalLandmarkCoordinates rdf:type owl:DatatypeProperty ;

            rdfs:label "AnatomicalLandmarkCoordinates"^^xsd:string ;

            dct:description "RECOMMENDED. Key:value pairs of any number of additional anatomical landmarks and their coordinates in voxel units (where first voxel has index 0,0,0) relative to the associated anatomical MRI, (e.g. {AC: [127,119,149], PC: [128,93,141], IH: [131,114,206]}, or {NAS: [127,213,139], LPA: [52,113,96], RPA: [202,113,91]})."^^xsd:string ;

            obo:IAO_0000116 "To be discussed"^^xsd:string .



###  http://purl.org/nidash/bids#InstitutionName

bids:InstitutionName rdf:type owl:DatatypeProperty ;

            rdfs:label "InstitutionName"^^xsd:string ;

            dct:description "RECOMMENDED. The name of the institution in charge of the equipment that produced the composite instances. Corresponds to DICOM Tag 0008, 0080 InstitutionName."^^xsd:string ;

