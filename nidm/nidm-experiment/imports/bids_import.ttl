bids:SoftwareVersions rdf:type owl:DatatypeProperty ;

            rdfs:label "SoftwareVersions"^^xsd:string ;

            dct:description "RECOMMENDED. Manufacturer’s designation of software version of the equipment that produced the composite instances. Corresponds to DICOM Tag 0018, 1020 Software Versions"^^xsd:string ;

            bids:SoftwareVersions owl:sameAs dicom:dicom_00181020 ;

            obo:IAO_0000116 "To be discussed"^^xsd:string .



###  http://purl.org/nidash/bids#HardcopyDeviceSoftwareVersion

bids:HardcopyDeviceSoftwareVersion rdf:type owl:DatatypeProperty ;

            rdfs:label "HardcopyDeviceSoftwareVersion"^^xsd:string ;

