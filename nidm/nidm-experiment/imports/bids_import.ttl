bids:ReferencesAndLinks rdf:type owl:AnnotationProperty ;

            rdfs:label "ReferencesAndLinks"^^xsd:string ;

            dct:description "OPTIONAL. List of references to publication that contain information on the dataset, or links."^^xsd:string ;

            obo:IAO_0000116 "To be discussed"^^xsd:string .



###  http://purl.org/nidash/bids#DatasetDOI

bids:DatasetDOI rdf:type owl:AnnotationProperty ;

            rdfs:label "DatasetDOI"^^xsd:string ;

