            rdfs:label "Manufacturer"^^xsd:string ;

            dct:description "RECOMMENDED. Manufacturer of the equipment that produced the composite instances. Corresponds to DICOM Tag 0008, 0070 Manufacturer"^^xsd:string ;

            bids:Manufacturer owl:sameAs dicom:dicom_00080070 ;

