            dct:description "OPTIONAL. Instructions how researchers using this dataset should acknowledge the original authors. This field can also be used to define a publication that should be cited in publications that use the dataset."^^xsd:string ;

            obo:IAO_0000116 "To be discussed"^^xsd:string .



###  http://purl.org/nidash/bids#Funding

bids:Funding rdf:type owl:AnnotationProperty ;

            rdfs:label "Funding"^^xsd:string ;

            dct:description "OPTIONAL. List of sources of funding (grant numbers)"^^xsd:string ;

