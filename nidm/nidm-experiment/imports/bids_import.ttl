bids:ScanningSequence rdf:type owl:DatatypeProperty ;

            rdfs:label "ScanningSequence"^^xsd:string ;

            dct:description "RECOMMENDED. Description of the type of data acquired. Corresponds to DICOM Tag 0018, 0020 Scanning Sequence."^^xsd:string ;

            bids:ScanningSequence owl:sameAs dicom:dicom_00180020 ;

            obo:IAO_0000116 "To be discussed"^^xsd:string .



