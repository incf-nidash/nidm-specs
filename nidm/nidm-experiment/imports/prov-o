            rdfs:label "Generation" ;
            
            rdfs:subClassOf :ActivityInfluence ,
                            :InstantaneousEvent ;
            
            :constraints "http://www.w3.org/TR/2013/REC-prov-constraints-20130430/#prov-dm-constraints-fig"^^xsd:anyURI ;
            
