:Revision rdf:type owl:Class ;
          
          rdfs:label "Revision" ;
          
          rdfs:subClassOf :Derivation ;
          
          :dm "http://www.w3.org/TR/2013/REC-prov-dm-20130430/#term-revision"^^xsd:anyURI ;
          
